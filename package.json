{
  "name": "amp-client",
  "version": "1.3.0-beta.0",
  "description": "JS Client for 68publishers/amp",
  "homepage": "http://www.68publishers.io/",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/68publishers/projects/amp-client"
  },
  "scripts": {
    "build:dev": "webpack --config webpack.config.dev.js --progress --profile",
    "build:prod": "webpack --config webpack.config.prod.js --progress --profile",
    "start:dev": "webpack --config webpack.config.dev.js --progress --profile && webpack-dev-server --config webpack.config.dev.js",
    "prepublish": "npm run build:prod",
    "eslint:fix": "eslint ./index.js ./src --ext .js --fix",
    "eslint": "eslint ./index.js ./src --ext .js"
  },
  "keywords": [
    "68publishers",
    "amp",
    "js-client",
    "amp-js-client"
  ],
  "author": "68publishers",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.22.7",
    "babel-loader": "^8.3.0",
    "eslint": "^8.44.0",
    "path": "^0.12.7",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "query-string": "^8.1.0"
  },
  "files": [
    "src",
    "index.js",
    "dist/amp-client.min.js",
    "dist/amp-client.standalone.min.js"
  ]
}
