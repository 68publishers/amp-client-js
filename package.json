    "babel-loader": "^8.0.6",
    "path": "^0.12.7",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "files": [
    "src",
    "index.js",
    "dist/amp-client.min.js",
    "dist/amp-client.standalone.min.js"
